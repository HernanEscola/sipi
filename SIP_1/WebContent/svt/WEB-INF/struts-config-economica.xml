			<forward name="altaCobro" path="/cCobroAltaAction.do?method=init" />
			<forward name="cobroByFactura" path="/cCobroBuscarAction.do?accion=refrescarCobrosByFactura" />
			<forward name="detalleCobro" path="/cCobroDetalleAction.do?method=init" />
			<forward name="altaCopia" path="/cFacturaAltaAction.do?method=init" />
