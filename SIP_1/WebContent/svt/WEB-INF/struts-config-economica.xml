			type="conicet.apps.siuvt.web.ingreso.action.CFacturaBuscarAction" validate="true">
			<forward name="formulario" path=".facturaBuscarDef" />
			<forward name="detalle" path="/cFacturaDetalleAction.do?method=init" />
			<forward name="detalleSaldoInicial" path="/cSaldoInicialDetalleAction.do?method=init" />
			<forward name="alta" path="/cFacturaAltaAction.do?method=init" />
		</action>
		<action name="cFacturaDetalleActionForm" parameter="btnSubmit" path="/cFacturaDetalleAction" scope="session"
