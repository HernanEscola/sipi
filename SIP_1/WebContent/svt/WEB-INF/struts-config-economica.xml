	<action-mappings>
		<action input=".cuentaAltaDef" name="cCuentaActionForm" path="/cCuentaAltaAction" scope="session"
			type="conicet.apps.svt.web.cuenta.action.CCuentaAltaAction" validate="true">
			<forward name="alta" path=".cuentaAltaDef" />
			<forward name="confirmar" path=".cuentaAltaDef" />
			<forward name="listado" path="/cCuentaBuscarAction.do?accion=refrescar" />
		</action>
		<action input=".cuentaBuscarDef" name="cCuentaBuscarActionForm" path="/cCuentaBuscarAction" scope="session"
			type="conicet.apps.svt.web.cuenta.action.CCuentaBuscarAction" validate="true">
			<forward name="formulario" path=".cuentaBuscarDef" />
			<forward name="listado" path=".cuentaBuscarDef" />
			<forward name="detalle" path="/cCuentaDetalleAction.do?accion=ini" />
			<forward name="alta" path="/cCuentaAltaAction.do?accion=ini" />
		</action>
		<action name="cCuentaDetalleActionForm" path="/cCuentaDetalleAction" scope="session" type="conicet.apps.svt.web.cuenta.action.CCuentaDetalleAction"
			validate="false">
			<forward name="formulario" path=".cuentaDetalleDef" />
			<forward name="listado" path=".cuentaBuscarDef" />
			<forward name="modificar" path="/cCuentaModificarAction.do?accion=ini" />
			<forward name="eliminar" path="/cCuentaEliminarAction.do?accion=ini" />
		</action>
		<action name="cCuentaEliminarActionForm" path="/cCuentaEliminarAction" scope="session" type="conicet.apps.svt.web.cuenta.action.CCuentaEliminarAction"
			validate="false">
			<forward name="eliminar" path=".cuentaEliminarDef" />
			<forward name="listado" path=".cuentaListarDef" />
			<forward name="volver" path=".cuentaDetalleDef" />
		</action>
		<action input=".cuentaModificarDef" name="cCuentaModificarActionForm" path="/cCuentaModificarAction" scope="session"
			type="conicet.apps.svt.web.cuenta.action.CCuentaModificarAction" validate="true">
			<forward name="modificar" path=".cuentaModificarDef" />
			<forward name="listado" path="/cCuentaBuscarAction.do?accion=refrescar" />
			<forward name="confirmar" path=".cuentaModificarDef" />
			<forward name="volver" path=".cuentaDetalleDef" />
		</action>
		<action input=".facturaAltaDef" parameter="btnSubmit" name="cFacturaAltaActionForm" path="/cFacturaAltaAction" scope="session"
			type="conicet.apps.siuvt.web.ingreso.action.CFacturaAltaAction" validate="true">
			<forward name="formulario" path=".facturaAltaDef" />
			<forward name="confirmar" path=".facturaAltaDef" />
			<forward name="listado" path=".facturaBuscarDef" />
			<forward name="listadoRecientes" path="/cFacturaBuscarAction.do?method=altasDelDia" />
			<forward name="cFacturaBuscarAction" path="/cFacturaBuscarAction.do?method=refrescar" />
			<forward name="cFacturaDetalleAction" path=".facturaDetalleDef" />
			<forward name="altaContratante" path="/cContratanteAltaAction.do?accion=ini" />
			<forward name="altaEntidad" path="/cEntidadAltaAction.do?accion=ini" />
			<forward name="seleccionarProductividad" path="/cSeleccionProductividadAction.do?method=init" />
			<forward name="seleccionarContraparte" path="/cSeleccionContraparteAction.do?method=init" />
		</action>

