			<forward name="detalleEgresoPRP" path="/cTransferenciaProductividadDetalleAction.do?method=init" />
		</action>
		<action input=".facturaEliminarDef" parameter="btnSubmit" name="cFacturaEliminarActionForm" path="/cFacturaEliminarAction" scope="session"
